#include "main.h"

/**
 * cap_string - helps to capitalize string
 * @s: strings to modify
 * Return: string
 */

char *cap_string(char *s)
{
  int x, y;
  char cp[13] = {' ', '\t', '\n', ',', ';', '.', '!', '?', '"', '(', ')', '{', '}'};

  for (x = 0; s[x] != '\0'; x++)
    {
      if (x == 0 && s[x] >= 'a' && s[x] <== 'z')
	{
	  s[x] -= 32;
	}
      for (y = 0; y < 13; y++)
	{
	  if (s[x + 1] >= 'a)
	}
    }
}