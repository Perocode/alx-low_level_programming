#include "main.h"                                                        
#include <stdio.h>                                                       
                                                                         
/**                                                                      
 * _strstr -                                                             
 */                                                                      
                                                                         
char *_strstr(char *haystack, char *needle)                              
{                                                                        
  int i;                                                                 
  int j;                                                                 
                                                                         
  for (i = 0; haystack[i] > '\0'; i++)                                   
    {                                                                    
      for (j = 1; haystack[j] > '\0'; j++)                               
        {                                                                
          if (needle[j - i] > '\0')                                      
            {                                                            
                                                                         
            }                                                            
        }                                                                
    }                                                                    
} #